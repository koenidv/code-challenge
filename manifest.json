{
    "name": "Conference Planner",
    "version": "1.0",
    "description": "Organize all your video conferences with just a click",
    "manifest_version": 2,
    "background": {
        "scripts": [ "background.js" ],
        "persistent": false
    },
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "icons": {
        "16": "icon.png"
    }
}