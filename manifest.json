{
    "name": "Conference Planner",
    "version": "0.1",
    "description": "Organize all your video conferences with just a few clicks",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js",
            "util/Conference.js",
            "util/database.js",
            "util/calendarsync.js",
            "lib/moment.min.js"
        ],
        "persistent": false
    },
    "permissions": [
        "contextMenus",
        "storage",
        "identity",
        "alarms",
        "notifications"
    ],
    "icons": {
        "16": "assets/16.png",
        "128": "assets/128.png",
        "256": "assets/256.png"
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "options_ui": {
        "page": "options/options.html",
        "chrome_style": true
    },
    "oauth2": {
        "client_id": "558579624647-p1bmhp0p8rvqsk1rvfuroak6aibjj8nc.apps.googleusercontent.com",
        "scopes": [
          "https://www.googleapis.com/auth/calendar.events"
        ]
      },
      "key": ""
}